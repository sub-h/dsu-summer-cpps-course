#include<stdio.h>
void main()
{
	int q,m,n,res,i,j,k;
	
	scanf("%d",&q);//no. of queries
	if(q>0 && q<=1000)
	{
		res=0;
		for(i=1;i<=q;i++)
		{
			scanf("%d",&n);
			scanf("%d",&m);
			for(j=1;j<=i;j++)
			{	
				if(n>0 && m>0)
				{
					for(k=1;k<=n;k++)
					{
						if(k%m==0)
						{
							res=res+(k%10);
						}
						else
							continue;
					}
				}
				else
				{
					printf("Invalid input\n");
				}
			}printf("%d\n",res);
		}
	}
	else
	{
		printf("No. of queries should be between 1 and 1000 only\n");
	}
}
